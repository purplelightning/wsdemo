{
  "name": "billapp",
  "version": "2.0.0",
  "main": "dist/electron/main/index.js",
  "description": "发票处理",
  "author": "youysong",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder"
  },
  "build": {
    "appId": "youysong",
    "productName": "发票处理",
    "copyright": "Copyright © 2022 Dada",
    "directories": {
      "output": "release/2.0.0"
    },
    "win": {
      "icon": "./public/star.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "installerIcon": "./public/star.ico",
      "uninstallerIcon": "./public/star.ico",
      "installerHeaderIcon": "./public/star.ico",
      "createDesktopShortcut": true,
      "shortcutName": "发票处理"
    }
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "pinia": "^2.0.17",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-electron": "^0.8.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.4",
    "vue-router": "^4.0.3"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@nuintun/qrcode": "^3.3.0",
    "adm-zip": "^0.5.9",
    "adm-zip-iconv": "^0.4.9",
    "nedb": "^1.8.0",
    "node-xlsx": "^0.21.0",
    "pdf-poppler": "^0.2.1"
  }
}
